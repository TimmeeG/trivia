{
  "compilerOptions": {
    "target": "es2015",
    "module": "es2015",
    "noEmit": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "experimentalDecorators": true,
    "jsx": "react-native",
    "lib": ["dom", "es2015", "es2016"],
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "noEmitHelpers": true,
    "importHelpers": true,
    "strict": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true
  }
}
